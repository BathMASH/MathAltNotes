1655 | [|][|]{|}| | %%     \includegraphics[width=0.32\textwidth]{tripleregionxyplane.svg}
1656 | [|][|]{|}| | %%     \caption{Projection of $G$ onto the $(x,y)$-plane}
1657 | [|][|]{|}| | %%     \label{tripleregionxyplane}
1658 | [|][|]{|}| | %%   \end{figure}
